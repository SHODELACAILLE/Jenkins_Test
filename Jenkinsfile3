pipeline {
        agent any 

    tools {
        // Sp√©cifier l'installation de Maven
        maven 'maven-3.5.2'
    }

  stage('SCM') {
    checkout scm
  }

  
  stage('SonarQube Analysis') {
    def mvn = tool 'Default Maven';
    withSonarQubeEnv() {
         dir('demo') {
      sh "./mvnw clean verify sonar:sonar -Dsonar.projectKey=com.example:demo -Dsonar.projectName='demo'"
    }
  }
}}