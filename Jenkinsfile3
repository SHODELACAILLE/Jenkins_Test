node {

        tools {
        // Sp√©cifier l'installation de Maven
        maven 'maven-3.5.2'
    }
  stage('SCM') {
    checkout scm
  }
  stage('SonarQube Analysis') {
    def mvn = tool 'Default Maven';
    withSonarQubeEnv() {
      sh "${mvn}/bin/mvn clean verify sonar:sonar -Dsonar.projectKey=SHODELACAILLE_Jenkins_Test_91b4e1cd-4dbd-4dab-bc82-4fcdd969ef3f -Dsonar.projectName='Jenkins_Test'"
    }
  }
}